{
    "name": "SmartNav",
    "version": "1.0",
    "description": "AR Navigation!",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js", "handsfree/handsfree.js"]
        }
    ],
    "background": {
        "page": "popup2.html"
    },

    "browser_action": {
        "default_popup": "popup.html"
    }

  }